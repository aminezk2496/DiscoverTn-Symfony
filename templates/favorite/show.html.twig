{% extends 'base-front.html.twig' %}

{% block title %}Favorite{% endblock %}

{% block body %}
    <h1>Favorite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdF</th>
                <td>{{ favorite.idF }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ favorite.user }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ favorite.createdAt ? favorite.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_favorite_index') }}">back to list</a>


    {{ include('favorite/_delete_form.html.twig') }}
{% endblock %}
